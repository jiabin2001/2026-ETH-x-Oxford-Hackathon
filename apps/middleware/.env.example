# ===== Core =====
FLARE_RPC_URL=https://coston2-api.flare.network/ext/C/rpc
CHAIN_ID=114
API_PORT=3001
TICK_SECONDS=10
AUDIT_PATH=./audit.jsonl

# ===== Optional LLM (OpenAI-compatible) =====
# Example: http://localhost:11434/v1/chat/completions (Ollama)
# Example: http://localhost:1234/v1/chat/completions (LM Studio)
LLM_ENABLED=false
LLM_CHAT_URL=
LLM_API_KEY=
LLM_MODEL=
LLM_TEMPERATURE=0.2
LLM_MAX_TOKENS=256

# ===== On-chain contracts (fill after deploy) =====
QUEUE_ADDRESS=
CONSTRAINTS_ADDRESS=

# ===== Keys =====
EXECUTOR_PRIVATE_KEY=

# ===== FDC (Flare Data Connector) =====
# mock | web2json
FDC_MODE=mock
FLARE_API_KEY=00000000-0000-0000-0000-000000000000

# Web2Json verifier base (trailing slash required)
WEB2JSON_VERIFIER_URL=https://jq-verifier-test.flare.rocks/verifier/jq/

# Data availability base (no trailing slash). Coston2:
FDC_DA_URL=https://ctn2-data-availability.flare.network

# FDC contracts on the chain (fill from Flare explorer / artifacts)
FDC_HUB_ADDRESS=
FDC_FEE_CONFIG_ADDRESS=

# ===== Web2Json inputs (replace with your NAV endpoint) =====
FDC_WEB2JSON_URL=https://swapi.info/api/people/3
FDC_WEB2JSON_JQ={name: .name, height: .height, mass: .mass, numberOfFilms: .films | length, uid: (.url | split("/") | .[-1] | tonumber)}
FDC_WEB2JSON_ABI={"components": [{"internalType": "string", "name": "name", "type": "string"},{"internalType": "uint256", "name": "height", "type": "uint256"},{"internalType": "uint256", "name": "mass", "type": "uint256"},{"internalType": "uint256", "name": "numberOfFilms", "type": "uint256"},{"internalType": "uint256", "name": "uid", "type": "uint256"}],"name": "task","type": "tuple"}
